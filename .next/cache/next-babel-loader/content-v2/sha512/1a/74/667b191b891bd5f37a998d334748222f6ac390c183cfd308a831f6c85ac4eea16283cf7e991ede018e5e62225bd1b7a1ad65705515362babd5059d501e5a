["import axios from \"axios\"\r\nimport { useEffect, useState } from \"react\"\r\n\r\nexport const useApiData = <T>(path: string, defaultValue: any): T => {\r\n  const [ data, setData ] = useState<T>(defaultValue)\r\n\r\n  useEffect(() => {\r\n    axios.get<T>(path).catch(err => err.response).then(response => {\r\n      setData(response.data)\r\n    })\r\n  }, [])\r\n\r\n  return data\r\n}\r\n\r\nexport const getCountries = <T>(path: string, defaultValue: any): T => {\r\n  const [ countries, setCountries ] = useState<T>(defaultValue)\r\n\r\n  useEffect(() => {\r\n    axios.get<T>(path).catch(err => err.response).then(response => {\r\n      console.log(\"response\",response)\r\n      setCountries(response.data)\r\n    })\r\n  }, [])\r\n\r\n  return countries\r\n}\r\n\r\nexport default useApiData\r\n"]},"metadata":{},"sourceType":"module"}